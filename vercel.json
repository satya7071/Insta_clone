{
  "version": 2,
  "builds": [
    { "src": "build.sh", "use": "@now/static-build" },
    { "src": "manage.py", "use": "@now/python" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "env": {
    "MYSQL_UNIX_PORT": "/tmp/mysql.sock"
  },
  "cleanUrls": true
}
